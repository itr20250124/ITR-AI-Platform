# 服務器配置
PORT=5000
NODE_ENV="development"
CORS_ORIGIN="http://localhost:3000"

# 文件上傳配置
MAX_FILE_SIZE=10485760  # 10MB
UPLOAD_DIR="uploads"

# 日誌配置
LOG_LEVEL="info"