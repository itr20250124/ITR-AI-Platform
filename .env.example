# 數據庫配置
DATABASE_URL="postgresql://username:password@localhost:5432/multi_ai_platform"

# Redis配置
REDIS_HOST="localhost"
REDIS_PORT=6379
REDIS_PASSWORD=""
REDIS_DB=0

# JWT配置
JWT_SECRET="your-super-secret-jwt-key-change-this-in-production"
JWT_EXPIRES_IN="7d"

# AI服務API密鑰
OPENAI_API_KEY="your-openai-api-key"
